{
  "name": "saros-dlmm-position-manager",
  "version": "0.18.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:unused": "eslint src --rule \"no-unused-vars: error\"",
    "lint:all": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict --noUnusedLocals --noUnusedParameters --noImplicitReturns",
    "code-quality": "npm run type-check:strict && npm run lint:unused",
    "test": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' jest --watchAll=false --runInBand",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' jest --watch --runInBand",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' jest --coverage --runInBand"
  },
  "dependencies": {
    "@pythnetwork/hermes-client": "^2.0.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
